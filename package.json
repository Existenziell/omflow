{
  "name": "omflow-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "dependencies": {
    "@fullcalendar/core": "^5.3.1",
    "@fullcalendar/daygrid": "^5.3.2",
    "axios": "^0.20.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "path": "^0.12.7",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "concurrently": "^5.3.0",
    "sass": "^1.26.11"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run clean && npm run build && npm run mkdirs && npm run copy:assets && npm run serve",
    "serve": "parcel serve src/index.html",
    "build": "parcel build src/index.html",
    "clean": "rm -rf ./.cache && rm -rf ./dist",
    "mkdirs": "mkdir -p dist/img/teachers/ && mkdir -p dist/img/layout/",
    "copy:assets": "cp -r src/img/teachers/* dist/img/teachers && cp -r src/img/layout/* dist/img/layout",
    "node": "concurrently --kill-others \"npm run sass\" \"nodemon server.js\"",
    "parcel": "npx parcel src/index.html src/*",
    "sass": "node-sass -w src/scss/ -o src/css/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Existenziell/omflow.git"
  },
